﻿@model WebApp.Models.Log
<!-- begin master section -->
<div class="row">
    <div class="col col-lg-12 col-md-12">
<!--begin log form  -->
<form id="log_form" class="smart-form" method="post" novalidate >
@Html.AntiForgeryToken()
  <input type="hidden" id="@Html.IdFor(model => model.Id)" 
                       name="@Html.NameFor(model => model.Id)" 
                       value="@Html.ValueFor(model => model.Id)" />
 <fieldset>
     <div class="row">
			              <!--主机名-->
            <section class="col col-3">
            <label class="label">@Html.DisplayNameFor(model => model.MachineName) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.MachineName)
            </label>
            </section>
 		 
	    
			              <!--时间-->
            <section class="col col-3">
            <label class="label">@Html.DisplayNameFor(model => model.Logged) </label>
            <label class="label">
            
			@Html.DisplayFor(model => model.Logged)
			    </label>
            </section>
			  		 
	    
			              <!--级别-->
            <section class="col col-3">
            <label class="label">@Html.DisplayNameFor(model => model.Level) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.Level)
            </label>
            </section>
 		 
	    
			              <!--信息-->
            <section class="col col-3">
            <label class="label">@Html.DisplayNameFor(model => model.Message) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.Message)
            </label>
            </section>
 		 
	   </div>
	    
     <div class="row">
			              <!--异常信息-->
            <section class="col col-3">
            <label class="label">@Html.DisplayNameFor(model => model.Exception) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.Exception)
            </label>
            </section>
 		 
	    
			              <!--事件属性-->
            <section class="col col-3">
            <label class="label">@Html.DisplayNameFor(model => model.Properties) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.Properties)
            </label>
            </section>
 		 
	    
			              <!--使用账号-->
            <section class="col col-3">
            <label class="label">@Html.DisplayNameFor(model => model.User) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.User)
            </label>
            </section>
 		 
	    
			              <!--日志-->
            <section class="col col-3">
            <label class="label">@Html.DisplayNameFor(model => model.Logger) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.Logger)
            </label>
            </section>
 		 
	   </div>
	    
     <div class="row">
			              <!--站点-->
            <section class="col col-3">
            <label class="label">@Html.DisplayNameFor(model => model.Callsite) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.Callsite)
            </label>
            </section>
 		 
	    
			  			  <!--已处理-->
              <section class="col col-3">
              <label class="label"><span class="required">@Html.DisplayNameFor(model => model.Resolved)</span> </label>
              <label class="label">
                @Html.DisplayFor(model => model.Resolved)
            </label>
            </section>
		 
	    
   </div>
	    
</fieldset>
</form>
<!-- end log form  -->
</div>
</div>
<!-- end master section -->
<!--========================================================================================-->


